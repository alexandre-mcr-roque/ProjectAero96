@{
    Layout = null;
    var summary = (FormSummary?)ViewBag.Summary;
}

@if (summary != null)
{
    <div class="alert alert-@(summary.AlertStyle) alert-dismissible mt-4" role="alert">
        @summary
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
}
